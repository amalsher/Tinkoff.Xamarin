    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="acq_btn_close">Close</string>
    <string name="acq_cant_delete_card_message">Unable to remove the card, try again later</string>
